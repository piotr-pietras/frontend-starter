{
  "name": "frontend-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm webpack --config webpack.config.js",
    "start": "pnpm ts-node ./server/index.ts",
    "prod": "pnpm run build & pnpm run start",
    "dev": "pnpm nodemon --watch ./src --ext ts,tsx --exec pnpm run build & pnpm run start"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.20",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
